alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias lh='ls -CFlh'

alias ls='ls -hF --color=auto'
alias lx='ll -BX'                   # sort by extension
alias lz='ll -rS'                   # sort by size
alias lt='ll -rt'                   # sort by date
alias lm='la | more'

#alias vi='vim'
alias ..='cd ..'
alias grep='grep --color'

alias show="ps -ef | grep "

alias rm="/home/liutao/bin/remove.sh"
